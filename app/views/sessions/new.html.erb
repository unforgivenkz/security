<% provide(:title, "Вход") %>
<% provide(:h1, "Вход") %>
<% provide(:img, "reg.jpg") %>
 <div class = "signup">
    <%= form_for(:session, url: sessions_path) do |f| %>

      <%= f.label :email %>
      <%= f.text_field :email %>

      <%= f.label :password, "Пароль" %>
      <%= f.password_field :password %>

      <%= f.submit "Войти"%>
      <p>Новый пользователь? <%= link_to "Пройдите регистрацию!", signup_path %></p>
    <% end %>
  </div>